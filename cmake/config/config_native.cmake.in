set(CMAKE_COMMAND_LINE "@ARGS_CMAKE_COMMAND_LINE@")
set(CMAKE_GENERATOR_PROCESS "@CMAKE_GENERATOR@")
message(STATUS "CMAKE_COMMAND_LINE = ${CMAKE_COMMAND_LINE}")
message(STATUS "CMAKE_GENERATOR = ${CMAKE_GENERATOR_PROCESS}")
execute_process(
  COMMAND "@CMAKE_COMMAND@" "-G${CMAKE_GENERATOR_PROCESS}"
  ${CMAKE_COMMAND_LINE}
  "@source_dir@"
  COMMAND_ERROR_IS_FATAL ANY)
