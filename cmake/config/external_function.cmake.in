set(MODULE "@ARGS_MODULE@")
set(FUNCTION "@ARGS_FUNCTION@")
set(ARGUMENTS "@ARGS_ARGUMENTS@")
message(DEBUG "EXTERNAL FUNCTION: ${FUNCTION}")
message(DEBUG "MODULE = ${MODULE}")
message(DEBUG "ARGUMENTS = ${ARGUMENTS}")
list(APPEND CMAKE_MODULE_PATH "@CMAKE_CURRENT_FUNCTION_LIST_DIR@")
if(MODULE)
  include(${MODULE})
endif()
@ARGS_FUNCTION@(${ARGUMENTS})
