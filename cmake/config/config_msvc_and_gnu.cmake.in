set(GNU_PATH "@GNU_PATH@")
if(GNU_PATH)
  set(ENV{PATH} "@GNU_PATH@\;$ENV{PATH}")
endif()
set(CMAKE_COMMAND_LINE "@ARGS_CMAKE_COMMAND_LINE@")
set(CMAKE_COMMAND_GENERATOR "@GNU_GENERATOR@")
if(NOT CMAKE_COMMAND_GENERATOR)
  set(CMAKE_COMMAND_GENERATOR "@CMAKE_GENERATOR@")
endif()
message(STATUS "CONFIG MSVC AND GNU")
message(STATUS "CMAKE_COMMAND_LINE = ${CMAKE_COMMAND_LINE}")
message(STATUS "CMAKE_COMMAND_GENERATOR = ${CMAKE_COMMAND_GENERATOR}")
execute_process(
  COMMAND "@CMAKE_COMMAND@" "-G${CMAKE_COMMAND_GENERATOR}"
  -DCMAKE_Fortran_COMPILER:PATH=@GNU_FORTRAN@
  -DBUILD_SHARED_LIBS=ON
  -DMSVC_AND_GNU_BUILD=ON
  -DCMAKE_GNUtoMS=ON
  ${CMAKE_COMMAND_LINE}
  "@source_dir@"
  COMMAND_ERROR_IS_FATAL ANY
)
