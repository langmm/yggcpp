include(CMakeFindDependencyMacro)
list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
include(AddYggInterface)
set(YGG_OPTIONS_SUPPORTED "@YGG_OPTIONS_SUPPORTED@")
set(YGG_COMMS_SUPPORTED "@YGG_COMMS_SUPPORTED@")
set(YGG_COMMS_AVAILABLE "@YGG_COMMS_AVAILABLE@")
set(YGG_LANGUAGES_SUPPORTED "@YGG_LANGUAGES_SUPPORTED@")
set(YGG_LANGUAGES_AVAILABLE "@YGG_LANGUAGES_AVAILABLE@")
set(YGG_INSTALL_DEPS "@YGG_INSTALL_DEPS@")
set(YGG_INSTALL_CONFIG "@YGG_INSTALL_CONFIG@")
foreach(dep IN LISTS YGG_INSTALL_DEPS)
  if (dep STREQUAL "Python3" OR dep STREQUAL "Python" OR
      dep STREQUAL "ZeroMQ" OR
      dep STREQUAL "rabbitmq-c")
    find_yggdrasil_dependency(${dep} FOR_PACKAGE_CONFIG)
  else()
    find_dependency(${dep})
  endif()
endforeach()
parse_yggdrasil_cmake_config()
include(${CMAKE_CURRENT_LIST_DIR}/YggInterfaceTargets.cmake)
if (TARGET YggInterface::YggInterface)
  add_library(YggInterface::CXX ALIAS YggInterface::YggInterface)
  add_library(YggInterface::C ALIAS YggInterface::YggInterface)
endif()
if (TARGET YggInterface::YggInterface_fortran)
  add_library(YggInterface::Fortran ALIAS
              YggInterface::YggInterface_fortran)
endif()
set(CMAKE_MODULE_PATH ${CMAKE_MODULE_PATH} CACHE INTERNAL "CMAKE_MODULE_PATH")
